{
  "name": "apollo-stem-academy",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=@apollo/web",
    "dev:api": "npm run dev --workspace=@apollo/api",
    "deploy:web": "npm run build --workspace=@apollo/web",
    "deploy:api": "npm run deploy --workspace=@apollo/api",
    "deploy": "npm run deploy --workspaces"
  }
}